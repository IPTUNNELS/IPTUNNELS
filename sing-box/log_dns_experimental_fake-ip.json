{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "Internet-dns",
        "type": "udp",
        "server": "9.9.9.9",
        "server_port": 53,
        "detour": "Internet"
      },
      {
        "tag": "direct-dns",
        "type": "local",
        "prefer_go": false
      },
      {
        "type": "fakeip",
        "tag": "remote-dns",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "domain_suffix": [
          ".lan"
        ],
        "action": "route",
        "server": "direct-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "network": "udp"
          },
          {
            "port": 443
          }
        ],
        "action": "predefined",
        "rcode": "NOERROR"
      },
      {
        "query_type": [
          "A",
          "AAAA",
          "HTTPS",
          "SVCB"
        ],
        "server": "remote-dns"
      }
    ],
    "strategy": "prefer_ipv4",
    "independent_cache": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "yacd",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "Internet",
      "default_mode": "rule"
    }
  }
}